{
    "$schema":  "./node_modules/@angular/cli/lib/config/schema.json",
    "version":  1,
    "cli":  {
                "packageManager":  "npm",
                "analytics":  "880e21a3-0f07-4c02-a625-14f083e9ca9b"
            },
    "newProjectRoot":  "projects",
    "projects":  {
                     "expense-tracker-frontend":  {
                                                      "projectType":  "application",
                                                      "schematics":  {

                                                                     },
                                                      "root":  "",
                                                      "sourceRoot":  "src",
                                                      "prefix":  "app",
                                                      "architect":  {
                                                                        "build":  {
                                                                                      "builder":  "@angular/build:application",
                                                                                      "options":  {
                                                                                                      "browser":  "src/main.ts",
                                                                                                      "tsConfig":  "tsconfig.app.json",
                                                                                                      "assets":  [
                                                                                                                     {
                                                                                                                         "glob":  "**/*",
                                                                                                                         "input":  "public"
                                                                                                                     }
                                                                                                                 ],
                                                                                                      "styles":  [
                                                                                                                     "src/material-theme.scss",
                                                                                                                     "src/styles.css"
                                                                                                                 ]
                                                                                                  },
                                                                                      "configurations":  {
                                                                                                             "production":  {
                                                                                                                                "budgets":  [
                                                                                                                                                {
                                                                                                                                                    "type":  "initial",
                                                                                                                                                    "maximumWarning":  "500kB",
                                                                                                                                                    "maximumError":  "1MB"
                                                                                                                                                },
                                                                                                                                                {
                                                                                                                                                    "type":  "anyComponentStyle",
                                                                                                                                                    "maximumWarning":  "4kB",
                                                                                                                                                    "maximumError":  "8kB"
                                                                                                                                                }
                                                                                                                                            ],
                                                                                                                                "outputHashing":  "all"
                                                                                                                            },
                                                                                                             "development":  {
                                                                                                                                 "optimization":  false,
                                                                                                                                 "extractLicenses":  false,
                                                                                                                                 "sourceMap":  true
                                                                                                                             }
                                                                                                         },
                                                                                      "defaultConfiguration":  "production"
                                                                                  },
                                                                        "serve":  {
                                                                                      "builder":  "@angular/build:dev-server",
                                                                                      "configurations":  {
                                                                                                             "production":  {
                                                                                                                                "buildTarget":  "expense-tracker-frontend:build:production"
                                                                                                                            },
                                                                                                             "development":  {
                                                                                                                                 "buildTarget":  "expense-tracker-frontend:build:development"
                                                                                                                             }
                                                                                                         },
                                                                                      "defaultConfiguration":  "development"
                                                                                  },
                                                                        "test":  {
                                                                                     "options":  {
                                                                                                     "runner":  "karma",
                                                                                                     "tsConfig":  "tsconfig.spec.json"
                                                                                                 },
                                                                                     "builder":  "@angular/build:unit-test"
                                                                                 }
                                                                    }
                                                  }
                 }
}
